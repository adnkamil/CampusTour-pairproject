<%- include('partials/header') %>
<div class="container">
    <form action="/universities/addUniversity" method="post">
        <div class="form-group">
            <label for="exampleInputEmail1">Name University</label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="campus" placeholder="e.g. Universitas Indonesia">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Address</label>
            <input type="text" class="form-control" id="exampleInputPassword1" name="address" placeholder="e.g. Depok, Jawa Barat"">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
    <%- include('partials/footer') %>

